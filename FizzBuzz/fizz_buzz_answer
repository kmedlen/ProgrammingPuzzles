first  = input('Enter 1st number: ')
second = input('Enter second number: ')

def recursion():
        count = range(first, second + 1)
        if count % 3 == 0 and count % 5 == 0:
                FB = "FizzBuzz"
                return FB
        elif count % 3 == 0:
                F = "Fizz"
                return F
        elif count % 5 == 0:
                B = "Buzz"
                return B
        else: #print count
                return count
print recursion()
